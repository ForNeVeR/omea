				<DispatchHandler ClassID="Omea.Request" Method="Action_Clip_Exec" />
			</Exec>
			<QueryStatus>
				<Constant Visible="1" Enabled="1" Checked="0" />
			</QueryStatus>
		</Action>
		<Action ID="ClipSilent">
			<Title Lang="en" Text="Clip and Save" />
			<InfoTip Lang="en" Text="Creates and saves an Omea clipping from the selected text." />
			<Image Normal="%JETIE%,2005" Hot="%JETIE%,1005" Disabled="%JETIE%,3005" />
			<Style Image="1" Text="1" />
