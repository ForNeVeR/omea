<Window x:Class="JetBrains.Omea.PresentationEx.Scheduller.TaskPickerWindow" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:sch="clr-namespace:JetBrains.Omea.PresentationEx.Scheduller" Title="JetBrains.Omea.PresentationEx.Scheduller" Height="300" Width="300">
	<DockPanel>
		<StackPanel DockPanel.Dock="Bottom" Orientation="Horizontal" HorizontalAlignment="Right">
			<Button Name="myBtnOk" Click="OnOk">OK</Button>
			<Button Name="myBtnCancel" Click="OnCancel">Cancel</Button>
		</StackPanel>
		<sch:TaskPicker x:Name="myPickerControl"></sch:TaskPicker>
	</DockPanel>
</Window>